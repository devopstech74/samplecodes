<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
    xsi:schemaLocation="http://www.springframework.org/schema/beans
    http://www.springframework.org/schema/beans/spring-beans.xsd
    http://www.springframework.org/schema/context
    http://www.springframework.org/schema/context/spring-context.xsd">
    <bean id="product" class="springproject.springbackend.Product">
    <property name="productName" value="Cofee"/>
    <property name="price" value="500"/>
    <property name="productid" value="1"/>
    </bean>
    <bean id="productDAO" class="springproject.springbackend.ProductDAOImpl">
    
    </bean>
    <bean id="supplier" class="springproject.springbackend.Supplier">
    <constructor-arg ref="product"/>
    </bean>
    
    <bean id="resource" class="springproject.springbackend.resource.Resource" factory-method="getInstance"/>
    
    
   </beans>